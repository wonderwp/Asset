{
  "name": "wonderwp/asset",
  "description": "WonderWp Asset Component",
  "type": "library",
  "keywords": ["asset", "WonderWp", "WordPress"],
  "homepage": "https://github.com/wonderwp/Asset",
  "license": "MIT",
  "authors": [
    {
      "name": "Jeremy Desvaux",
      "homepage": "https://jdmweb.com"
    },
    {
      "name": "WonderWp Community",
      "homepage": "http://wonderwp.net/Contribution/Contributors.html"
    }
  ],
  "require": {
    "php": ">=7.1",
    "wonderwp/logging": "^1.0",
    "wonderwp/service": "^1.0",
    "symfony/asset": "5.3.4"
  },
  "autoload": {
    "psr-4": {
      "WonderWp\\Component\\Asset\\": "src",
      "WonderWp\\Component\\Asset\\Tests\\": "tests"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "minimum-stability": "dev",
  "require-dev": {
    "phpunit/phpunit": "9.5.7",
    "johnpbloch/wordpress": "^5.8",
    "wonderwp/functions": "dev-develop"
  }
}
